
body
  user-select: none
  margin: 0
  padding: 0
  overflow: hidden
  width: 100%
  height: 100%

#App, #Scene, #World
  background: #EFE
  width: 100%
  height: 100%
  margin: 0
  padding: 0

#World
  position: absolute
  font-family: monospace

  span
    display: none

  ul
    margin: 0
    padding: 0
    li
      list-style-type: none
      margin: 0
      padding: 0

@font-face
  font-family: Munro
  src: url(../../../assets/munro.ttf)
